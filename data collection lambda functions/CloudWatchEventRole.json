{
    "Name": "ScheduleLambdaFunctions",
    "Description": "Schedule Lambda functions for data collection",
    "ScheduleExpression": "cron(0 0 * * ? *)", 
    "State": "ENABLED",
    "Targets": [
      {
        "Id": "linkedin",
        "Arn": "arn:aws:lambda:us-east-1:123456789012:function:linkedin"
      },
      {
        "Id": "glassdoor",
        "Arn": "arn:aws:lambda:us-east-1:123456789012:function:gassdoor"
      },
      {
        "Id": "indeed",
        "Arn": "arn:aws:lambda:us-east-1:123456789012:function:indeed"
      }
    ]
  }
  